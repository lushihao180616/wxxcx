.row {
  padding-left: 20rpx;
  padding-right: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-radius: 10rpx;
  background-color: #efefef;
  height: 90rpx;
  margin-bottom: 5rpx;
}

.text {
  font-size: 30rpx;
  width: 200rpx;
}

.picker {
  font-size: 30rpx;
  color: green;
  width: 100%;
}

.input {
  width: 100%;
  font-size: 30rpx;
}

.placeholderstyle {
  font-size: 30rpx;
}

.foot {
  width: 100%;
  height: 90rpx;
  position: fixed;
  bottom: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.addaddresstext {
  width: 100%;
  height: 90rpx;
  background-color: #09BB07;
  font-size: 30rpx;
  color: white;
  font-weight: bold;
  padding-top: 10rpx;
}