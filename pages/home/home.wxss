page {
  height: 100%;
  background-color: #efefef;
}

.imgs {
  margin-left: 25rpx;
  margin-right: 25rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-bottom: 20rpx;
}

.img {
  width: 100%;
}

.top, .bottom {
  height: 36%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.shape {
  width: 45%;
  border: 5rpx solid #efefef;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: "宋体";
  font-size: 40rpx;
  font-weight: bold;
  color: white;
  border-radius: 20rpx;
}

.middle {
  height: 24%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.middle_middle {
  width: 70%;
  border: 5rpx solid #efefef;
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: "宋体";
  font-size: 40rpx;
  font-weight: bold;
  color: white;
  border-radius: 20rpx;
  background-color: #F4A460;
}

#left_top {
  background-color: #fd8e72;
}

#right_top {
  background-color: #fed437;
}

#left_bottom {
  background-color: #54e0c7;
}

#right_bottom {
  background-color: greenyellow;
}
