.all {
  height: 100%;
  background-color: #efefef;
}
.head {
  width: 750rpx;
  height: 80rpx;
  background-color: bisque;
  position: fixed;
  top: 0rpx;
  display: flex;
  flex-direction: row;
}

.icon {
  height: 60rpx;
  width: 230rpx;
  display: flex;
  flex-direction: row;
}

.imageshape {
  margin: 10rpx;
  width: 60rpx;
  height: 60rpx;
}

.search {
  height: 60rpx;
  border-radius: 30rpx;
  background-color: white;
  width: 100%;
  margin: 10rpx;
  display: flex;
  flex-direction: row;
}

.imageshape2 {
  margin: 10rpx;
  width: 40rpx;
  height: 40rpx;
}

.input {
  margin-top: 3rpx;
  width: 100%;
  font-size: 30rpx;
}

.body {
  margin-top: 90rpx;
}

.item {
  height: 180rpx;
  margin: 15rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
}

.item_row1,.item_row2 {
  display: flex;
  flex-direction: row;
}

.item_type {
  width: 0rpx;
  height: 0rpx;
  border-top: 50rpx solid #f58220;
  border-left: 50rpx solid #f58220;
  border-bottom: 50rpx dashed white;
  border-right: 50rpx dashed white;
  margin-left: -3rpx;
  margin-top: -3rpx;
  display: flex;
  align-items: center;
}

.item_type_image {
  width: 54rpx;
  height: 54rpx;
  margin-left: -40rpx;
  margin-top: -40rpx;
}

.item_name_time {
  height: 70rpx;
  width: 100%;
  margin-top: 20rpx;
  margin-left: -30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.item_name {
  color: #444;
  font-weight: bold;
}

.item_time {
  width: 120rpx;
  height: 50rpx;
  color: white;
  display: flex;
  margin-top: -18rpx;
  justify-content: center;
  font-weight: bold;
  background-color: #f58220;
  border-left: 5rpx solid white;
  border-bottom: 5rpx solid white;
}

.item_address_price {
  height: 70rpx;
  width: 100%;
  display: flex;
  margin: 10rpx;
  margin-top: -10rpx;
  padding-top: 10rpx;
  flex-direction: row;
  justify-content: space-between;
  border-top: 1rpx dashed #aaa;
}

.item_address {
  font-size: 36rpx;
  line-height: 50rpx;
  margin-top: 10rpx;
  margin-left: 200rpx;
}

.item_price {
  font-size: 50rpx;
  font-weight: bold;
  color: red;
}