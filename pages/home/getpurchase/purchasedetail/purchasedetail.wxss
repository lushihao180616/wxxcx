.page {
  background-color: #efefef;
  height: 300rpx;
}

.row,.row2 {
  padding-left: 20rpx;
  padding-right: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-radius: 10rpx;
  background-color: #efefef;
  height: 75rpx;
  margin-bottom: 5rpx;
}

.text {
  font-size: 30rpx;
  width: 200rpx;
}

.text_value,.textarea_value {
  font-size: 30rpx;
  width: 100%;
}

.row2 {
  height: 150rpx;
}

.textarea_value {
  margin-top: 10rpx;
  height: 140rpx;
}

.foot {
  width: 100%;
  height: 90rpx;
  position: fixed;
  bottom: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.addaddresstext {
  width: 100%;
  height: 90rpx;
  background-color: #09BB07;
  font-size: 30rpx;
  color: white;
  font-weight: bold;
  padding-top: 10rpx;
}